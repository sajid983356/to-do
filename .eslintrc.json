{
    "env": {
      "browser": true,
      "es2021": true
    },
    "extends": [
      "react-app",
      "plugin:react/recommended"
    ],
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true
      },
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "plugins": [
      "react",
      "import",
      "jsx-a11y",
      "react-hooks"
    ],
    "rules": {
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off"
    }
  }
  